{
  "name": "noc",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev": "NODE_ENV=dev tsnd --respawn --env-file=.env.dev src/app.ts",
    "docker:test": "docker compose -f docker-compose-test.yml --env-file .env.test up -d",
    "start": "NODE_ENV=prod npm run build && node dist/app.js",
    "test:coverage": "npm run test && jest --coverage",
    "test:watch": "npm run test && jest --watch",
    "test": "NODE_ENV=test npm run docker:test && jest --env-file=.env.test"
  },
  "engines": {
    "npm": ">=10.5",
    "node": ">=21"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/nodemailer": "^6.4.14",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "5.10.2",
    "cron": "^3.1.6",
    "dotenv": "^16.4.5",
    "env-var": "^7.4.1",
    "mongoose": "^8.2.0",
    "node-mailer": "^0.1.1",
    "nodemailer": "^6.9.10",
    "prisma": "^5.10.2"
  }
}
